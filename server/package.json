{
  "name": "halobattles-server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "types": "./dist/index.d.ts",
  "imports": {
    "#procedure/*": "./dist/lib/procedures/*",
    "#lib/*": "./dist/lib/*"
  },
  "scripts": {
    "mirgate": "node ./scripts/migrate.js",
    "make": "node ./scripts/index.js",
    "build": "pnpm tsc",
    "dev:compile": "tsc --watch",
    "dev:run": "nodemon ./dist/index.js --config ./nodemon.json",
    "test:run": "nodemon --config ./nodemon.test.json",
    "test:compile": "tsc --watch",
    "start": "node ./dist/index.js",
    "dev": "pnpm --parallel --stream run /^dev:.*/",
    "test": "pnpm --parallel --stream run /^test:.*/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@trpc/server": "^10.45.0",
    "dotenv": "^16.3.1",
    "halobattles-shared": "workspace:^",
    "jose": "^5.2.0",
    "lodash.groupby": "^4.6.0",
    "lodash.remove": "^4.7.0",
    "piscina": "^4.3.0",
    "sqlite3": "^5.1.7",
    "superjson": "^2.2.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.39.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.remove": "^4.7.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.4",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3"
  }
